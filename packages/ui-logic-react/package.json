{
  "name": "ui-logic-react",
  "version": "0.1.4",
  "license": "MIT",
  "description": "Separate your UI logic from your UI I/O - React bindings",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "prepare": "tsc",
    "prepare:watch": "npm run prepare -- -w",
    "test": "mocha --require ts-node/register \"ts/**/*.test.ts\"",
    "test:watch": "mocha -r source-map-support/register -r ts-node/register \"ts/**/*.test.ts\" --watch --watch-extensions ts"
  },
  "keywords": [
    "ui",
    "logic",
    "react",
    "single-responsibilty"
  ],
  "author": "Vincent den Boer",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShishKabab/ui-logic.git"
  },
  "dependencies": {
    "events": "^3.0.0",
    "react": "^16.8.6",
    "source-map-support": "^0.4.18"
  },
  "devDependencies": {
    "@types/events": "^1.2.0",
    "@types/expect": "^1.20.4",
    "@types/mocha": "^2.2.44",
    "@types/node": "^10.12.11",
    "@types/react": "^16.8.19",
    "chai": "^4.1.2",
    "expect": "^23.5.0",
    "mocha": "^4.0.1",
    "react": "^16.0.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1",
    "ui-logic-core": "^0.1.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "ui-logic-core": "^0.1.1"
  }
}
